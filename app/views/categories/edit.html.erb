<h3>Editing category</h3>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@category) do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name %>
         <br/>
        <%= f.submit "Update category", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>